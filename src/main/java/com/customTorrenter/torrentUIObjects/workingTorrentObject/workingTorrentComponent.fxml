<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.HBox?>


<HBox fx:id="hbox" alignment="CENTER_LEFT" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <CheckBox fx:id="checkbox" mnemonicParsing="false" style="-fx-padding: 0 15;" />
      <SplitPane dividerPositions="0.5" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefHeight="40.0" prefWidth="843.0" HBox.hgrow="ALWAYS">
         <items>
            <JFXTextField fx:id="name" editable="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefColumnCount="5" text="text" />
            <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="20.0">
               <children>
                  <Button fx:id="stop_resume_Btn" mnemonicParsing="false" text="yes" />
                  <HBox alignment="CENTER">
                     <children>
                        <JFXTextField fx:id="downloadDone" editable="false" minWidth="-Infinity" prefHeight="25.0" prefWidth="51.0" text="text" />
                        <JFXTextField fx:id="torrentSize" editable="false" minWidth="-Infinity" prefHeight="25.0" prefWidth="45.0" text="text" />
                     </children>
                  </HBox>
                  <ProgressIndicator fx:id="progressPercent" progress="0.0" />
                  <JFXButton fx:id="cancelButton" />
               </children>
            </HBox>
         </items>
      </SplitPane>
   </children>
</HBox>
